<h1>Ingresar al Sistema</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :correo %><br />
    <%= text_field_tag :correo, params[:correo] %>
  </p>
  <p>
    <%= label_tag :contrasenia %><br />
    <%= password_field_tag :contrasenia %>
  </p>
  <p class="button"><%= submit_tag "Log in" %></p>
<% end %>